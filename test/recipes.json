{\n  \"recipes\": [\n    {\n      \"title\": \"Peach and Blueberry Yogurt Parfait\",\n      \"ingredient_list\": [\n        \"2 peaches, sliced\",\n        \"1 cup blueberries\",\n        \"1 cup yogurt\",\n        \"2 tablespoons orange juice\"\n      ],\n      \"difficulty\": 1,\n      \"cooking_time\": \"10 minutes\",\n      \"cooking_steps\": [\n        \"In a bowl, mix the yogurt with the orange juice until well combined.\",\n        \"In serving glasses, layer the yogurt mixture, sliced peaches, and blueberries.\",\n        \"Repeat the layers until the glasses are filled.\",\n        \"Serve immediately or chill in the refrigerator for a few minutes before serving.\"\n      ]\n    },\n    {\n      \"title\": \"Chicken and Bell Pepper Wrap\",\n      \"ingredient_list\": [\n        \"4 sandwich wraps\",\n        \"200g chicken slices\",\n        \"1 red bell pepper, sliced\",\n        \"1 yellow bell pepper, sliced\",\n        \"2 tablespoons yogurt (for dressing)\"\n      ],\n      \"difficulty\": 2,\n      \"cooking_time\": \"15 minutes\",\n      \"cooking_steps\": [\n        \"In a skillet over medium heat, warm the chicken slices for about 5 minutes until heated through.\",\n        \"In a bowl, mix the sliced red and yellow bell peppers with yogurt to create a dressing.\",\n        \"Lay out the sandwich wraps and distribute the warmed chicken slices evenly among them.\",\n        \"Top with the bell pepper mixture and roll the wraps tightly.\",\n        \"Cut in half and serve.\"\n      ]\n    },\n    {\n      \"title\": \"Banana and Peach Pie\",\n      \"ingredient_list\": [\n        \"2 ripe bananas, mashed\",\n        \"2 peaches, sliced\",\n        \"1 pre-made pie crust\",\n        \"1 tablespoon lemon juice\",\n        \"1 tablespoon sugar (optional)\"\n      ],\n      \"difficulty\": 3,\n      \"cooking_time\": \"40 minutes\",\n      \"cooking_steps\": [\n        \"Preheat the oven to 375°F (190°C).\",\n        \"In a mixing bowl, combine the mashed bananas, sliced peaches, lemon juice, and sugar until well mixed.\",\n        \"Pour the fruit mixture into the pie crust, spreading it evenly.\",\n        \"Bake in the preheated oven for 25-30 minutes, or until the crust is golden and the fruit is bubbling.\",\n        \"Allow the pie to cool for 10 minutes before slicing and serving.\"\n      ]\n    }\n  ]\n}
